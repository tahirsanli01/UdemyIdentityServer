<!--begin::Timeline widget 3-->
<div class="card h-md-100">
	<!--begin::Header-->
	<div class="card-header border-0 pt-5">
		<h3 class="card-title align-items-start flex-column">
			<span class="card-label fw-bold text-dark">@Model.Title</span>
			<span class="text-muted mt-1 fw-semibold fs-7">Toplam: @Model.TaskForWeeks.Count()</span>
		</h3>
		<!--begin::Toolbar-->
		<div class="card-toolbar">
			<a href="#" class="btn btn-sm btn-light">Report Cecnter</a>
		</div>
		<!--end::Toolbar-->
	</div>
	<!--end::Header-->
	<!--begin::Body-->
	<div class="card-body pt-7 px-0">
		<!--begin::Nav-->
		<ul class="nav nav-stretch nav-pills nav-pills-custom nav-pills-active-custom d-flex justify-content-between mb-8 px-5">
			@foreach (var item in Model.DayOfWeeks)
			{
				@if (item.CurrentDate.ToShortDateString() == DateTime.Now.ToShortDateString())
				{
					<li class="nav-item p-0 ms-0">
						<a class="nav-link btn d-flex flex-column flex-center rounded-pill min-w-45px py-4 px-3 btn-active-danger active" data-bs-toggle="tab" href="#kt_timeline_widget_3_tab_content_@item.DayOfWeekString">
							<span class="fs-7 fw-semibold">@item.DayOfWeekShortString</span>
							<span class="fs-6 fw-bold">@item.DayOfWeek</span>
						</a>
					</li>
				}
				else if (Model.TaskForWeeks.Any(x => x.TaskStartDateTime <= item.CurrentDate && x.TaskEndDateTime >= item.CurrentDate))
				{
					<li class="nav-item p-0 ms-0 ">
						<a class="nav-link btn d-flex flex-column flex-center rounded-pill min-w-45px py-4 px-3 btn-warning text-white" data-bs-toggle="tab" href="#kt_timeline_widget_3_tab_content_@item.DayOfWeekString">
							<span class="fs-7 fw-semibold text-white">@item.DayOfWeekShortString</span>
							<span class="fs-6 fw-bold text-white">@item.DayOfWeek</span>
						</a>
					</li>
				}
				else
				{
					<li class="nav-item p-0 ms-0">
						<a class="nav-link btn d-flex flex-column flex-center rounded-pill min-w-45px py-4 px-3 btn-active-danger" data-bs-toggle="tab" href="#kt_timeline_widget_3_tab_content_@item.DayOfWeekString">
							<span class="fs-7 fw-semibold">@item.DayOfWeekShortString</span>
							<span class="fs-6 fw-bold">@item.DayOfWeek</span>
						</a>
					</li>
				}

			}
		</ul>
		<!--end::Nav-->
		<!--begin::Tab Content (ishlamayabdi)-->
		<div class="tab-content mb-2 px-9">
			@foreach (var item in Model.DayOfWeeks)
			{
				<div class='tab-pane @(item.CurrentDate.ToShortDateString()==DateTime.Now.ToShortDateString()?"active":"")' id="kt_timeline_widget_3_tab_content_@item.DayOfWeekString">
				@foreach (var taskItem in Model.TaskForWeeks.Where(x => x.TaskStartDateTime <= item.CurrentDate && x.TaskEndDateTime >= item.CurrentDate))
				{
						<div class="d-flex align-items-center mb-6">
							<span data-kt-element="bullet" class="bullet bullet-vertical d-flex align-items-center min-h-70px mh-100 me-4 bg-success"></span>
							<div class="flex-grow-1 me-5">
								<div class="text-gray-800 fw-semibold fs-2">
									@taskItem.TaskStartTime - @taskItem.TaskEndTime
									<span class="text-gray-400 fw-semibold fs-7">AM</span>
								</div>
								<div class="text-gray-700 fw-semibold fs-6">@taskItem.Title</div>
								<div class="text-gray-400 fw-semibold fs-7">
									Danışman
									<a href="#" class="text-primary opacity-75-hover fw-semibold">@taskItem.PersonFullName</a>
								</div>
							</div>
							<a href="#" class="btn btn-sm btn-light" data-bs-toggle="modal" data-bs-target="#kt_modal_create_project">Detay</a>
						</div>
					}
				</div>
			}
		</div>
		<!--end::Tab Content-->
		<!--begin::Action-->
		<div class="float-end d-none">
			<a href="#" class="btn btn-sm btn-light me-2" data-bs-toggle="modal" data-bs-target="#kt_modal_create_project">Add Lesson</a>
			<a href="#" class="btn btn-sm btn-danger" data-bs-toggle="modal" data-bs-target="#kt_modal_create_app">Call Sick for Today</a>
		</div>
		<!--end::Action-->
	</div>
	<!--end: Card Body-->
</div>
